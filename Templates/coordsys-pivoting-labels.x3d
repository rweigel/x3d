    <!--First position and rotate viewpoint into positive-X-Y-Z octant using a Transform-->
    <Transform rotation='0 1 0 0.758' translation='10 5 10'>
      <Viewpoint description='crossSections of Torus' orientation='1 0 0 -0.3' position='0 0 0'/>
    </Transform>
    <ProtoInstance name='ExtrusionCrossSection'>
      <fieldValue name='name' value='Torus'/>
