      <field accessType='initializeOnly' name='creaseAngle' type='SFFloat'/>
      <field accessType='initializeOnly' name='solid' type='SFBool'/>
      <field accessType='inputOutput' name='lineColor' type='MFColor'/>
      <field accessType='inputOutput' name='crossSectionMaterial' type='SFNode'/>
      <field accessType='inputOutput' name='extrusionMaterial' type='SFNode'/>
      <field accessType='initializeOnly' name='extrusionImageTexture' type='SFNode'/>
      <field accessType='initializeOnly' name='extrusionTextureTransform' type='SFNode'/>
      <!--run-time eventIn Extrusion-modification interfaces for VRML 97 compatibility-->
      <field accessType='inputOnly' name='set_crossSection' type='MFVec2f'/>
      <field accessType='inputOnly' name='set_spine' type='MFVec3f'/>
      <field accessType='inputOnly' name='set_scale' type='MFVec2f'/>
      <field accessType='inputOnly' name='set_orientation' type='MFRotation'/>
      <field accessType='initializeOnly' name='traceEnabled' type='SFBool'/>
    </ExternProtoDeclare>
    <!--====================-->
